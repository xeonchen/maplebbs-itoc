/* ­ì²z¦P f_img.c, °ß¤@¤£¦P³B¬° size + 1 (¦w¥þ·j´M¦r¦ê¥Î) */

#include <fcntl.h>
#include <sys/stat.h>


char *
f_simg(fpath, fsize)
  char *fpath;
  int *fsize;
{
  int fd, size, len;
  char *fw;
  struct stat st;

  if ((fd = open(fpath, O_RDONLY)) < 0)
    return NULL;

  fpath = NULL;

  if (!fstat(fd, &st) && S_ISREG(st.st_mode) && (size = st.st_size) > 0
    && (fpath = (char *) malloc(size + 1)))
  {
    *fsize = size + 1;
    fw = fpath;

    while (len = read(fd, fw, size))
      fw += len;

    *fw = '\0';

    if (fw - fpath != size)
    {
      free(fpath);
      fpath = NULL;
    }
  }

  close(fd);
  return fpath;
}

